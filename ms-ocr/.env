# application.properties (Python equivalent - .env file)
# Service Configuration
SERVICE_NAME=ms-bill-ocr
SERVICE_PORT=5000
SERVICE_HOST=0.0.0.0

# Java Backend Integration
JAVA_API_URL=http://localhost:8083/api/bill/bill/addbill
JAVA_API_TIMEOUT=10

